{
  "headers": [
    {
      "source": "/sitemap-new.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" }
      ]
    }
  ],
  "routes": [
    { "src": "/sitemap-new.xml", "dest": "public/sitemap-new.xml" },
    { "src": "/robots.txt", "dest": "public/robots.txt" },
    { "src": "/google.*\\.html", "dest": "public/google.html" },
    { "src": "/assets/(.*)", "dest": "public/assets/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
